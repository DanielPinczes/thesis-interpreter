let factorial = fn(x) {
       if (x == 0) {
           return 1;
       } else {
           return x * factorial(x - 1);
       }
    };
let result = factorial(5);
let anotherVar = 10;
let add = fn(a, b) { return a + b; };
let sum = add(result, anotherVar);
if (sum > 50) {
   return true;
}
